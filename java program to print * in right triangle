class RightTrianglePattern   
{   
 public static void main(String args[])   
    {   
        
    //row denotes the number of rows you want to print  
 int i, j, row=6;   
    //outer loop for rows  
 for(i=0; i<row; i++)   
    {   
    //inner loop for columns  
    for(j=0; j<=i; j++)   
    {      
    System.out.print("* ");   
    }   
    System.out.println();   
 }   
 }   
 } 
